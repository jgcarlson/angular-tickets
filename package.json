{
  "name": "tickets",
  "version": "0.0.0",
  "scripts": {
    "dev:start:server": "nodemon --watch 'server/**/*.ts' --ignore 'server/**/*.test.ts' --exec 'ts-node --project server/tsconfig.server.json' server/index.ts",
    "dev:start:app": "ng serve",
    "dev:build:app": "ng build --watch",
    "dev": "concurrently \"npm run dev:start:server\" \"npm run dev:build:app\"",
    "test:app": "ng test",
    "lint:app": "ng lint",
    "e2e": "ng e2e",
    "prod:build:server": "tsc --project server/tsconfig.server.json",
    "prod:build:app": "ng build --prod --aot --build-optimizer --vendor-chunk=true --optimization --extract-licenses",
    "prod:build": "NODE_ENV=production npm run prod:build:app && NODE_ENV=production npm run prod:build:server",
    "start": "NODE_ENV=production node dist/server/index.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.1",
    "core-js": "^2.5.4",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "material-design-icons": "^3.0.1",
    "mongoose": "^5.2.16",
    "mongoose-paginate": "^5.0.3",
    "rxjs": "~6.2.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "errorhandler": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
